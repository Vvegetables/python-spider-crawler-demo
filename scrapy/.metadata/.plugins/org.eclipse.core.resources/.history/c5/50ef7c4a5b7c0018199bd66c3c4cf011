import scrapy


class TutorialItem(scrapy.Item):
    # define the fields for your item here like:
    # name = scrapy.Field()
    pass

class LogoItem(scrapy.Item):
    date = scrapy.Field() #获得数据的时间点
    name = scrapy.Field() #数据的名字
    url = scrapy.Field() #图片的url地址
    address = scrapy.Field() #网址
