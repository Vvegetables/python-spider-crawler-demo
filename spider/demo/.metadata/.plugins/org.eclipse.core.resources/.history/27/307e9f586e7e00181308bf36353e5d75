#coding=utf-8
from openpyxl.workbook.workbook import Workbook
from openpyxl.reader.excel import load_workbook

def excel_read(path):
    wb = load_workbook(path)
    sheet = wb.active
    return sheet
#     max_row = sheet.max_row
